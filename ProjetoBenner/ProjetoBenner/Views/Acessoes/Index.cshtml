@model IEnumerable<ProjetoBenner.Models.Acesso>

@{
    ViewBag.Title = "Index";
     if(Session["Tipo"].ToString() != "ADM")
    {
         Response.Redirect("~/Login/Index");
    }
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tipo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Codigo_Acesso }) |
            @Html.ActionLink("Details", "Details", new { id=item.Codigo_Acesso }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Codigo_Acesso })
        </td>
    </tr>
}

</table>
